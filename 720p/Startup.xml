<window id="2999">
	<defaultcontrol always="true">10</defaultcontrol>
	<allowoverlay>no</allowoverlay>
	<controls>
		<control type="button" id="10">
			<include condition="Skin.HasSetting(Use_Startup_Playlist)">StartupPlaylist</include>
			<include condition="Skin.HasSetting(NextAired)">NextAiredScript</include>
			<include condition="Skin.HasSetting(Qlock)">QlockScript</include>
			<include condition="Skin.HasSetting(HomeRandomMovies) | Skin.HasSetting(HomeRandomTVShows) | Skin.HasSetting(HomeRandomAlbums)">randomitemsscript</include>
			<onfocus>ReplaceWindow(Home)</onfocus>
			<texturenofocus>-</texturenofocus>
			<texturefocus>-</texturefocus>
		</control>
	</controls>
</window>
