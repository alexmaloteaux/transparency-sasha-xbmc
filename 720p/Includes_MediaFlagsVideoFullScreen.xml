<includes>
	<include name="FullScreenVideoMediaFlagsResolution">
		<posx>0</posx>
		<posy>0</posy>
		<width>64</width>
		<height>48</height>
	</include>
	<include name="FullScreenVideoMediaFlagsAspectRatio">
		<posx>68</posx>
		<posy>0</posy>
		<width>64</width>
		<height>48</height>
	</include>
	<include name="FullScreenVideoMediaFlagsVideoCodec">
		<posx>136</posx>
		<posy>0</posy>
		<width>64</width>
		<height>48</height>
	</include>
	<include name="FullScreenVideoMediaFlagsAudioCodec">
		<posx>204</posx>
		<posy>0</posy>
		<width>64</width>
		<height>48</height>
	</include>
	<include name="FullScreenVideoMediaFlagsRating">
		<posx>272</posx>
		<posy>0</posy>
		<width>64</width>
		<height>48</height>
	</include>
	<include name="FullScreenVideoMediaFlagsStudio">
		<posx>346</posx>
		<posy>6</posy>
		<width>52</width>
		<height>35</height>
	</include>
	<include name="FullScreenVideoMediaFlags">
	<!-- Resolution -->
		<control type="image">
			<include>FullScreenVideoMediaFlagsResolution</include>
			<texture fallback="mediaflag-resolution.png">flags/movies/resolution/$INFO[VideoPlayer.VideoResolution,,.png]</texture>
		</control>
	<!-- Aspect Ratio -->
		<control type="image">
			<include>FullScreenVideoMediaFlagsAspectRatio</include>
			<texture fallback="mediaflag-aspectratio.png">flags/movies/aspectratio/$INFO[VideoPlayer.VideoAspect,,.png]</texture>
		</control>
	<!-- Video Codec -->
		<control type="image">
			<include>FullScreenVideoMediaFlagsVideoCodec</include>
			<texture fallback="mediaflag-videocodec.png">flags/movies/videocodec/$INFO[VideoPlayer.VideoCodec,,.png]</texture>
		</control>
	<!-- Audio Codec -->
		<control type="image">
			<include>FullScreenVideoMediaFlagsAudioCodec</include>
			<texture>flags/movies/audiocodec/dd10.png</texture>
			<visible>[stringcompare(VideoPlayer.AudioCodec,ac3) | stringcompare(VideoPlayer.AudioCodec,ac-3)] + stringcompare(VideoPlayer.AudioChannels,1)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsAudioCodec</include>
			<texture>flags/movies/audiocodec/dd20.png</texture>
			<visible>[stringcompare(VideoPlayer.AudioCodec,ac3) | stringcompare(VideoPlayer.AudioCodec,ac-3)] + stringcompare(VideoPlayer.AudioChannels,2)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsAudioCodec</include>
			<texture>flags/movies/audiocodec/dd30.png</texture>
			<visible>[stringcompare(VideoPlayer.AudioCodec,ac3) | stringcompare(VideoPlayer.AudioCodec,ac-3)] + stringcompare(VideoPlayer.AudioChannels,3)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsAudioCodec</include>
			<texture>flags/movies/audiocodec/dd50.png</texture>
			<visible>[stringcompare(VideoPlayer.AudioCodec,ac3) | stringcompare(VideoPlayer.AudioCodec,ac-3)] + stringcompare(VideoPlayer.AudioChannels,5)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsAudioCodec</include>
			<texture>flags/movies/audiocodec/dd51.png</texture>
			<visible>[stringcompare(VideoPlayer.AudioCodec,ac3) | stringcompare(VideoPlayer.AudioCodec,ac-3)] + stringcompare(VideoPlayer.AudioChannels,6)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsAudioCodec</include>
			<texture>flags/movies/audiocodec/dd61.png</texture>
			<visible>[stringcompare(VideoPlayer.AudioCodec,ac3) | stringcompare(VideoPlayer.AudioCodec,ac-3)] + stringcompare(VideoPlayer.AudioChannels,7)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsAudioCodec</include>
			<texture>flags/movies/audiocodec/dd71.png</texture>
			<visible>[stringcompare(VideoPlayer.AudioCodec,ac3) | stringcompare(VideoPlayer.AudioCodec,ac-3)] + stringcompare(VideoPlayer.AudioChannels,8)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsAudioCodec</include>
			<texture>flags/movies/audiocodec/dts10.png</texture>
			<visible>[stringcompare(VideoPlayer.AudioCodec,dca) | stringcompare(VideoPlayer.AudioCodec,dts)] + stringcompare(VideoPlayer.AudioChannels,1)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsAudioCodec</include>
			<texture>flags/movies/audiocodec/dts20.png</texture>
			<visible>[stringcompare(VideoPlayer.AudioCodec,dca) | stringcompare(VideoPlayer.AudioCodec,dts)] + stringcompare(VideoPlayer.AudioChannels,2)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsAudioCodec</include>
			<texture>flags/movies/audiocodec/dts51.png</texture>
			<visible>[stringcompare(VideoPlayer.AudioCodec,dca) | stringcompare(VideoPlayer.AudioCodec,dts)] + stringcompare(VideoPlayer.AudioChannels,6)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsAudioCodec</include>
			<texture>flags/movies/audiocodec/dts61.png</texture>
			<visible>[stringcompare(VideoPlayer.AudioCodec,dca) | stringcompare(VideoPlayer.AudioCodec,dts)] + stringcompare(VideoPlayer.AudioChannels,7)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsAudioCodec</include>
			<texture>flags/movies/audiocodec/dts71.png</texture>
			<visible>[stringcompare(VideoPlayer.AudioCodec,dca) | stringcompare(VideoPlayer.AudioCodec,dts)] + stringcompare(VideoPlayer.AudioChannels,8)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsAudioCodec</include>
			<texture fallback="mediaflag-audiocodec.png">flags/movies/audiocodec/$INFO[VideoPlayer.AudioCodec,,.png]</texture>
			<visible>![stringcompare(VideoPlayer.AudioCodec,ac3) | stringcompare(VideoPlayer.AudioCodec,ac-3) | stringcompare(VideoPlayer.AudioCodec,dca) | stringcompare(VideoPlayer.AudioCodec,dts)]</visible>
		</control>
	<!-- Rating US -->
		<control type="image">
			<include>FullScreenVideoMediaFlagsRating</include>
			<texture>flags/movies/rating/rating-g.png</texture>
			<visible>VideoPlayer.Content(Movies) + substring(VideoPlayer.Mpaa,Rated G) | substring(VideoPlayer.Mpaa,Rated: G)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsRating</include>
			<texture>flags/movies/rating/rating-pg.png</texture>
			<visible>VideoPlayer.Content(Movies) + [substring(VideoPlayer.Mpaa,Rated PG) | substring(VideoPlayer.Mpaa,Rated: PG)] + !substring(VideoPlayer.Mpaa,Rated PG-13) + !substring(VideoPlayer.Mpaa,Rated: PG-13)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsRating</include>
			<texture>flags/movies/rating/rating-pg13.png</texture>
			<visible>VideoPlayer.Content(Movies) + substring(VideoPlayer.Mpaa,Rated PG-13) | substring(VideoPlayer.Mpaa,Rated: PG-13)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsRating</include>
			<texture>flags/movies/rating/rating-r.png</texture>
			<visible>VideoPlayer.Content(Movies) + substring(VideoPlayer.Mpaa,Rated R) | substring(VideoPlayer.Mpaa,Rated: R)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsRating</include>
			<texture>flags/movies/rating/rating-nc17.png</texture>
			<visible>VideoPlayer.Content(Movies) + substring(VideoPlayer.Mpaa,Rated NC-17) | substring(VideoPlayer.Mpaa,Rated: NC-17)</visible>
		</control>
	<!-- Rating UK -->
		<control type="image">
			<include>FullScreenVideoMediaFlagsRating</include>
			<texture>flags/movies/rating/bbfcU.png</texture>
			<visible>VideoPlayer.Content(Movies) + StringCompare(ListItem.Mpaa,UK:U)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsRating</include>
			<texture>flags/movies/rating/bbfcUC.png</texture>
			<visible>VideoPlayer.Content(Movies) + StringCompare(ListItem.Mpaa,UK:UC)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsRating</include>
			<texture>flags/movies/rating/bbfcPG.png</texture>
			<visible>VideoPlayer.Content(Movies) + StringCompare(ListItem.Mpaa,UK:PG)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsRating</include>
			<texture>flags/movies/rating/bbfc12.png</texture>
			<visible>VideoPlayer.Content(Movies) + StringCompare(ListItem.Mpaa,UK:12)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsRating</include>
			<texture>flags/movies/rating/bbfc12A.png</texture>
			<visible>VideoPlayer.Content(Movies) + StringCompare(ListItem.Mpaa,UK:12A)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsRating</include>
			<texture>flags/movies/rating/bbfc15.png</texture>
			<visible>VideoPlayer.Content(Movies) + StringCompare(ListItem.Mpaa,UK:15)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsRating</include>
			<texture>flags/movies/rating/bbfc18.png</texture>
			<visible>VideoPlayer.Content(Movies) + StringCompare(ListItem.Mpaa,UK:18)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsRating</include>
			<texture>flags/movies/rating/bbfcR18.png</texture>
			<visible>VideoPlayer.Content(Movies) + StringCompare(ListItem.Mpaa,UK:R18)</visible>
		</control>
	<!-- Rating DE -->
		<control type="image">
			<include>FullScreenVideoMediaFlagsRating</include>
			<texture>flags/movies/rating/fsk0.png</texture>
			<visible>VideoPlayer.Content(Movies) + substring(VideoPlayer.Mpaa,FSK0) | substring(VideoPlayer.Mpaa,FSK 0) | substring(VideoPlayer.Mpaa,FSK o.A.)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsRating</include>
			<texture>flags/movies/rating/fsk6.png</texture>
			<visible>VideoPlayer.Content(Movies) + substring(VideoPlayer.Mpaa,FSK6) | substring(VideoPlayer.Mpaa,FSK 6)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsRating</include>
			<texture>flags/movies/rating/fsk12.png</texture>
			<visible>VideoPlayer.Content(Movies) + substring(VideoPlayer.Mpaa,FSK12) | substring(VideoPlayer.Mpaa,FSK 12)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsRating</include>
			<texture>flags/movies/rating/fsk16.png</texture>
			<visible>VideoPlayer.Content(Movies) + substring(VideoPlayer.Mpaa,FSK16) | substring(VideoPlayer.Mpaa,FSK 16)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsRating</include>
			<texture>flags/movies/rating/fsk18.png</texture>
			<visible>VideoPlayer.Content(Movies) + substring(VideoPlayer.Mpaa,FSK18) | substring(VideoPlayer.Mpaa,FSK 18)</visible>
		</control>
	<!-- Rating FR -->
		<control type="image">
			<include>FullScreenVideoMediaFlagsRating</include>
			<texture>flags/movies/rating/csa10.png</texture>
			<visible>VideoPlayer.Content(Movies) + substring(VideoPlayer.mpaa,10 ans)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsRating</include>
			<texture>flags/movies/rating/csa12.png</texture>
			<visible>VideoPlayer.Content(Movies) + substring(VideoPlayer.mpaa,12 ans)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsRating</include>
			<texture>flags/movies/rating/csa16.png</texture>
			<visible>VideoPlayer.Content(Movies) + substring(VideoPlayer.mpaa,16 ans)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsRating</include>
			<texture>flags/movies/rating/csa18.png</texture>
			<visible>VideoPlayer.Content(Movies) + substring(VideoPlayer.mpaa,18 ans)</visible>
		</control>
	<!-- Rating TV -->
		<control type="image">
			<include>FullScreenVideoMediaFlagsRating</include>
			<texture>flags/movies/rating/tv-y.png</texture>
			<visible>VideoPlayer.Content(Episodes) + substring(VideoPlayer.Mpaa,TV-Y) + ![substring(VideoPlayer.Mpaa,TV-Y7) | substring(VideoPlayer.Mpaa,TV-Y7-FV)]</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsRating</include>
			<texture>flags/movies/rating/tv-y7.png</texture>
			<visible>VideoPlayer.Content(Episodes) + substring(VideoPlayer.Mpaa,TV-Y7) | substring(VideoPlayer.Mpaa,TV-Y7-FV)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsRating</include>
			<texture>flags/movies/rating/tv-g.png</texture>
			<visible>VideoPlayer.Content(Episodes) + substring(VideoPlayer.Mpaa,TV-G)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsRating</include>
			<texture>flags/movies/rating/tv-pg.png</texture>
			<visible>VideoPlayer.Content(Episodes) + substring(VideoPlayer.Mpaa,TV-PG)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsRating</include>
			<texture>flags/movies/rating/tv-14.png</texture>
			<visible>VideoPlayer.Content(Episodes) + substring(VideoPlayer.Mpaa,TV-14)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsRating</include>
			<texture>flags/movies/rating/tv-ma.png</texture>
			<visible>VideoPlayer.Content(Episodes) + substring(VideoPlayer.Mpaa,TV-MA)</visible>
		</control>
		<control type="image">
			<include>FullScreenVideoMediaFlagsRating</include>
			<texture>mediaflag-rating.png</texture>
			<visible>IsEmpty(VideoPlayer.Mpaa)</visible>
		</control>
	<!-- Studio Background-->
		<control type="image">
			<posx>340</posx>
			<posy>0</posy>
			<width>64</width>
			<height>48</height>
			<texture>mediaflag.png</texture>
		</control>
	<!-- Studio -->
		<control type="image">
			<include>FullScreenVideoMediaFlagsStudio</include>
			<texture fallback="mediaflag-studio.png">flags/movies/studio/$INFO[VideoPlayer.Studio,,.png]</texture>
			<visible>Player.HasMedia + !VideoPlayer.Content(MusicVideos)</visible>
		</control>
	<!-- Studio MusicVideos-->
		<control type="image">
			<include>FullScreenVideoMediaFlagsStudio</include>
			<texture fallback="mediaflag-studio.png">flags/music/recordlabel/$INFO[VideoPlayer.Studio,,.png]</texture>
			<visible>Player.HasMedia + VideoPlayer.Content(MusicVideos)</visible>
		</control>
	</include>
</includes>
