<window>
	<defaultcontrol>100</defaultcontrol>
	<controls>
		<control type="image">
			<posx>0</posx>
			<posy>0</posy>
			<width>1280</width>
			<height>720</height>
			<texture>black.png</texture>
			<animation effect="fade" start="0" end="70" time="300">WindowOpen</animation>
			<animation effect="fade" start="70" end="0" time="300">WindowClose</animation>
		</control>
		<control type="group">
			<include>dialogeffect</include>
			<!-- ** Required ** Do not change id or type -->
			<control type="button">
				<posx>1240</posx>
				<posy>20</posy>
				<width>20</width>
				<height>20</height>
				<texturefocus>close-window-focus.png</texturefocus>
				<texturenofocus>close-window-nofocus.png</texturenofocus>
				<onclick>PreviousMenu</onclick>
				<visible>Skin.HasSetting(TouchScreenMode)</visible>
			</control>
			<control type="label">
				<description>heading label</description>
				<posx>40</posx>
				<posy>15</posy>
				<width>1200</width>
				<height>30</height>
				<align>center</align>
				<aligny>center</aligny>
				<textcolor>blue</textcolor>
				<font>font-32</font>
				<label>$INFO[Control.GetLabel(20)]</label>
			</control>
			<!-- ** Required ** Do not change <description> or <type> -->
			<control type="label" id="20">
				<description>Showtimes Label1</description>
				<visible>false</visible>
			</control>
			<control type="image">
				<posx>0</posx>
				<posy>54</posy>
				<width>1280</width>
				<height>2</height>
				<texture>separator.png</texture>
			</control>
			<!-- ** Required ** Do not change <description> or <type> -->
			<control type="label" id="30">
				<description>Showtimes Label2</description>
				<posx>0</posx>
				<posy>90</posy>
				<width>1280</width>
				<align>center</align>
				<font>font-20</font>
				<label>-</label>
				<textcolor>lightblue</textcolor>
				<shadowcolor>black</shadowcolor>
			</control>
			<control type="label">
				<posx>0</posx>
				<posy>115</posy>
				<width>1280</width>
				<align>center</align>
				<font>font-20</font>
				<label>$INFO[Control.GetLabel(40)]$INFO[Control.GetLabel(50), - ]</label>
				<textcolor>lightblue</textcolor>
				<shadowcolor>black</shadowcolor>
			</control>
			<!-- ** Required ** Do not change <description> or <type> -->
			<control type="label" id="40">
				<description>Showtimes Label3</description>
				<visible>false</visible>
			</control>
			<!-- ** Required ** Do not change <description> or <type> -->
			<control type="label" id="50">
				<description>Showtimes Label4</description>
				<visible>false</visible>
			</control>
			<control type="image">
				<posx>200</posx>
				<posy>145</posy>
				<width>880</width>
				<height>2</height>
				<texture>separator.png</texture>
			</control>
			<!-- ** Required ** Do not change <id> or <type> -->
			<control type="list" id="100">
				<description>Showtimes List</description>
				<posx>200</posx>
				<posy>190</posy>
				<width>880</width>
				<height>420</height>
				<scrolltime>200</scrolltime>
				<pagecontrol>61</pagecontrol>
				<onup>100</onup>
				<ondown>100</ondown>
				<onleft>61</onleft>
				<onright>61</onright>
				<itemlayout width="880" height="70">
					<control type="image">
						<width>880</width>
						<height>70</height>
						<posx>0</posx>
						<posy>0</posy>
						<texture border="7">list-nofocus.png</texture>
					</control>
					<control type="label">
						<posx>10</posx>
						<posy>3</posy>
						<width>860</width>
						<height>20</height>
						<font>font-20</font>
						<textcolor>blue</textcolor>
						<aligny>center</aligny>
						<align>left</align>
						<info>ListItem.Label</info>
					</control>
					<control type="label">
						<posx>710</posx>
						<posy>3</posy>
						<width>860</width>
						<height>20</height>
						<font>font-20</font>
						<textcolor>blue</textcolor>
						<aligny>center</aligny>
						<align>right</align>
						<info>ListItem.Thumb</info>
					</control>
					<control type="label">
						<posx>10</posx>
						<posy>23</posy>
						<width>860</width>
						<height>20</height>
						<font>font-20</font>
						<aligny>center</aligny>
						<align>left</align>
						<info>ListItem.ActualIcon</info>
					</control>
					<control type="label">
						<posx>10</posx>
						<posy>43</posy>
						<width>860</width>
						<height>20</height>
						<font>font-20</font>
						<textcolor>lightblue</textcolor>
						<aligny>center</aligny>
						<align>left</align>
						<info>ListItem.Label2</info>
					</control>
				</itemlayout>
				<focusedlayout width="880" height="70">
					<control type="image">
						<width>880</width>
						<height>70</height>
						<posx>0</posx>
						<posy>0</posy>
						<texture border="7">list-nofocus.png</texture>
					</control>
					<control type="image">
						<width>880</width>
						<height>70</height>
						<posx>0</posx>
						<posy>0</posy>
						<texture border="7">list-focus2.png</texture>
						<visible>Control.HasFocus(100)</visible>
						<include>ListFocusScroll21</include>
					</control>
					<control type="label">
						<posx>10</posx>
						<posy>3</posy>
						<width>860</width>
						<height>20</height>
						<font>font-20</font>
						<textcolor>blue</textcolor>
						<aligny>center</aligny>
						<align>left</align>
						<info>ListItem.Label</info>
					</control>
					<control type="label">
						<posx>710</posx>
						<posy>3</posy>
						<width>860</width>
						<height>20</height>
						<font>font-20</font>
						<textcolor>blue</textcolor>
						<aligny>center</aligny>
						<align>right</align>
						<info>ListItem.Thumb</info>
					</control>
					<control type="label">
						<posx>10</posx>
						<posy>23</posy>
						<width>860</width>
						<height>20</height>
						<font>font-20</font>
						<aligny>center</aligny>
						<align>left</align>
						<info>ListItem.ActualIcon</info>
					</control>
					<control type="label">
						<posx>10</posx>
						<posy>43</posy>
						<width>860</width>
						<height>20</height>
						<font>font-20</font>
						<textcolor>lightblue</textcolor>
						<aligny>center</aligny>
						<align>left</align>
						<info>ListItem.Label2</info>
					</control>
				</focusedlayout>
			</control>
			<control type="image">
				<posx>0</posx>
				<posy>655</posy>
				<width>1280</width>
				<height>2</height>
				<texture>separator.png</texture>
			</control>
			<control type="spincontrol" id="61">
				<description>Next page button</description>
				<posx>1180</posx>
				<posy>660</posy>
				<subtype>page</subtype>
				<font>font-19</font>
				<onleft>61</onleft>
				<onright>61</onright>
				<ondown>100</ondown>
				<onup>100</onup>
				<textcolor>white</textcolor>
				<showonepage>true</showonepage>
			</control>
		</control>
	</controls>
</window>
