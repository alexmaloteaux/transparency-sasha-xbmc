<window id="2005">
 <defaultcontrol>-</defaultcontrol>
	<controls>
		<!-- codec infos -->
		<include condition="Skin.HasSetting(HateTransparency)">T-Underlay43</include>
		<control type="image">
			<description>media info background image</description>
			<posx>0</posx>
			<posy>5</posy>
			<width>1280</width>
			<height>130</height>
			<texture border="0,11,0,11">underlay4.png</texture>
		</control>
		<control type="label" id="10">
			<description>row 1 label</description>
			<posx>50</posx>
